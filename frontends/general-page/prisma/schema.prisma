datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
  relationMode = "prisma"
}

generator client {
  provider        = "prisma-client-js"
}

model User {
  id        Int     @id @default(autoincrement())
  email     String  @unique
  password  String
}

model Feature {
  id          Int      @id @default(autoincrement())
  title       String
  description String?
  demo        String?
  large       Boolean? @default(false)
}